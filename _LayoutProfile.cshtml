                     <li>@Html.ActionLink("Applicant", "Index", "Applicant")</li>
                                       
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
              <hr />
             <div class="container">
                 <div class="row-fluid">
                     <div class="span4">
                         @* @Html.Action("RDashbourd","Reservation")*@
                     </div>
                     <div class="span8">
                         @RenderBody()
                     </div>
                 </div>
             </div>
             </div>
         </div>
</body>
</html>

