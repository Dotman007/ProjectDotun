<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - MyProfile</title>
    >
    <script src="../../Scripts/jquery-1.9.1.min.js")" type="text/javascript"></script>
    <script src="../../Scripts/bootstrap.js"></script>
    <script src="../../Scripts/bootstrap.min.js"></script>
    <link href="../../Controllers/Content/default.css" rel="stylesheet" />
    <link href="../../Controllers/Content/Site.css" rel="stylesheet" />
    <link href="../../Controllers/Content/bootstrap.css" rel="stylesheet" />
    <link href="../../Controllers/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="../../Controllers/Content/bootstrap-responsive.css" rel="stylesheet" />
    <link href="../../Controllers/Content/bootstrap-responsive.min.css" rel="stylesheet" />

    <script type="text/javascript">
        $(document).ready(function () {
            $('.carousel').carousel({
                interval: 3000
            });
        });
    </script>
</head>
<body data-spy="scroll" data-target=".bs-docs-sidebar" style="background-color:white;">
     <div class="container" style="background-color: white;">
         <div class="row-fluid">
                         <div style=" background-color:white;" class="span12">
                <div class="row-fluid">
                    <div class="span12">
                        <div class="span6">
                            <img src="../../Images/exoticlogo.png" />
                        </div>
                        <div class="span6">
                             <div style="float: right; margin-top:10px; background-color:white; color:white;" class="btn">
                                @Html.Partial("_LogOnPartial")
                            </div>
                        </div>
                    </div>
                </div>
                @*<div class="span12">*@
                    <div class="navbar navbar-inverse">
                        <div class="navbar-inner">
                            <div class="container">
                                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <div class="nav-collapse collapse" style="font-family:Verdana;font-size:x-large">
